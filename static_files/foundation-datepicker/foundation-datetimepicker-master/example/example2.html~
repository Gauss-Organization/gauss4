	<div class="nine columns">
          <h2>Example</h2>
          <p>Attached to a field with the format specified via options.</p>
          <div class="panel">
            <input type="text" value="02-16-2012 23:00" id="dp1" >
          </div>
          
        </div>
      

	<script>
		$(function(){

			$('#dp1').fdatetimepicker({
				format: 'mm-dd-yyyy hh:ii'
			});
					
		});
	</script>

